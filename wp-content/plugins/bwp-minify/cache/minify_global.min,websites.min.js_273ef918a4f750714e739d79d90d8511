jQuery(document).ready(function(b){function a(c,d){newWidth=c.outerWidth();newPosX=c.position().left+parseInt(c.css("margin-left"));b("#header-nav-indicator").stop().animate({left:newPosX,width:newWidth},d)}$curMenuItem=b("#menu-main-navigation li.current-menu-item");if($curMenuItem.length==0){$curMenuItem=b("#menu-main-navigation li.current-page-ancestor");if($curMenuItem.length==0){$curMenuItem=b("#menu-main-navigation #menu-item-400")}}b("#header-main-navigation").append('<div id="header-nav-indicator" />');$curMenuItem.css("border-bottom","none");a($curMenuItem,0);setTimeout(function(){a($curMenuItem,0)},800);b("#menu-main-navigation li").live("mouseover mouseout",function(c){if(c.type=="mouseover"){a(b(this),"medium")}if(c.type=="mouseout"){a($curMenuItem,"medium")}})});
;jQuery(document).ready(function(a){a(".website").parent("article").height(a(this).height());a(".website").css("height","100px").hover(function(){a(this).stop().animate({height:a(this).find("img").outerHeight()},1000,function(){a(this).find("h1").fadeIn("fast")})},function(){a(this).find("h1").fadeOut("fast");a(this).stop().animate({height:"100px"},1000)}).find("h1").fadeOut(0)});